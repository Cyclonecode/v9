/console.log/ { return PRINT; }
/var/ { return VAR; }
/if/ { return IF; }
/while/ { return WHILE; }
/for/ { return FOR; }
/in/ { return IN; }
/true/ { return TRUE; }
/false/ { return FALSE; }
/function/ { return FUNCTION; }
/this/ { return THIS; }
/new/ { return NEW; }

/[_a-zA-Z][a-zA-Z0-9_]*/ { lval.s = yylex.Text(); return ID }
/'(\\.|[^\\']+)*'|\"(\\.|[^\\"]+)*\"/ { lval.s = yylex.Text(); return STRING }

"=="  { return COMP_EQU; }
"!="  { return COMP_NEQU; }
"<"   { return COMP_LESS; }
"<="  { return COMP_LTE; }
">"   { return COMP_GTR; }
">="  { return COMP_GTE; }
"===" { return COMP_SEQU; }
"!==" { return COMP_SNEQU; }

"&&" { return BOOL_AND; }
"||" { return BOOL_OR; }

/[ \t\n]/  { /* Skip blanks and tabs. */ }
/[0-9]+\.[0-9]*|\.[0-9]+|[0-9]+/ { lval.s = yylex.Text(); return NUM }
/./ { return int(yylex.Text()[0]) }
